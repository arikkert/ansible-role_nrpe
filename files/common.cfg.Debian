# File managed by Ansible
command[check_swap]=/usr/lib/nagios/plugins/check_swap -w 20% -c 10% -n ok
command[check_disk_root]=/usr/lib/nagios/plugins/check_disk -w 15% -c 10% -p /
command[check_disk_boot]=/usr/lib/nagios/plugins/check_disk -w 20% -c 10% -p /boot
command[check_disk_home]=/usr/lib/nagios/plugins/check_disk -w 7% -c 5% -p /home
command[check_dns_remote]=/usr/lib/nagios/plugins/check_dns -H google.com
command[check_smtp]=/usr/lib/nagios/plugins/check_smtp -H localhost
command[check_uptime]=/usr/lib/nagios/plugins/check_uptime
command[check_service_kubelet]=/usr/lib/nagios/plugins/check_service.sh -s kubelet
command[check_service_containerd]=/usr/lib/nagios/plugins/check_service.sh -s containerd
